{
  "expo": {
    "name": "Fahrgut",
    "slug": "Fahrgut",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.rezaafg.Fahrgut"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "904705b7-740b-4d61-b315-508e95b08beb"
      }
    },
    "splash": {
      "image": "./assets/logo7.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  }
}
